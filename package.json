{
  "name": "@ezthera/node-librealsense",
  "version": "0.2130.1",
  "description": "Node.js API for Intel® RealSense™ SDK 2.0",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ezthera/prebuilt-node-librealsense.git"
  },
  "keywords": [
    "realsense",
    "camera",
    "depth camera"
