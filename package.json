  },
  "devDependencies": {
    "jsdoc": "^3.6.6",
    "marked": "~0.3.9",
    "cmake-js": "^7.0.0"
  },
  "types": "./index.d.ts",
  "bugs": {
    "url": "https://github.com/Ezthera/prebuilt-node-librealsense/issues"
  },
  "homepage": "https://github.com/Ezthera/prebuilt-node-librealsense#readme",
